{
  "name": "beanstalk-dashboard-for-chrome",
  "description": "beanstalk-dashboard-for-chrome",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "js/background.js"
      ]
    }
  },
  "permissions": [
    "system.network"
  ],  
  "sockets": {
      "tcp": {
        "connect": ""
      }
    },
  "icons": { 
    "16": "img/icon_16.png",
    "32": "img/icon_32.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  }
}